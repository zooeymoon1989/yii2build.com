RewriteEngine on
 #YiiSettings
 RewriteEngine On
 RewriteBase /
 RewriteCond %{REQUEST_FILENAME} !-f
 RewriteCond %{REQUEST_FILENAME} !-d
 RewriteRule ^(.*)\?*$ index.php/$1 [L,QSA]


 #PHP error
 php_flag display_startup_errors on
 php_flag display_errors on
 php_flag html_errors on
 php_flag  log_errors on


